include_rules

CXXFLAGS = -Wall -Werror -std=c++11

: foreach minimizers.yaggo generate_sequence.yaggo |> !yaggo |> {cmdlines}
: foreach minimizers.cc compute_minimizers.cc generate_sequence.cc misc.cc | {cmdlines} |> !cxx |>

: misc.o minimizers.o compute_minimizers.o |> !lxxd |> minimizers
: misc.o generate_sequence.o |> !lxxd |> generate_sequence

